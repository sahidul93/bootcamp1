<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Cigna Automation Script " parallel="methods" thread-count="4">

    <parameter name="platform" value="mac"/>
    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="53.0"/>
    <parameter name="url" value="https://www.cigna.com"/>

    <test name=" Cigna Home">
        <classes>

            <class name="CignaHomePageTest"/>
           <!--<class name="CignaNavicationTabTest"/>
            <class name="CignaHealthCovTest"/>
            <class name="CignaGlobalProductTest"/>-->


        </classes>
    </test>
</suite>
